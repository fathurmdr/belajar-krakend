{
  "extra_config": {
    "security/cors": {
      "allow_origins": ["http://nginx-web.frprojects.site"],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ],
      "allow_headers": [
        "Accept",
        "Accept-Encoding",
        "Accept-Language",
        "App-Version",
        "Authorization",
        "Cache-Control",
        "Content-Length",
        "Content-Type",
        "Device-Name",
        "Origin",
        "User-Agent",
        "X-Real-IP",
        "X-Forwarded-For",
        "X-Forwarded-Host",
        "X-Forwarded-Port",
        "X-Forwarded-Proto",
        "X-Api-Key",
        "X-Auth-Token",
        "X-Requested-With",
        "X-AOP-Brand",
        "X-AOP-Device-Id",
        "X-AOP-Device-Name",
        "X-AOP-IP-Address",
        "X-AOP-MAC-Address",
        "X-AOP-Manufacturer",
        "X-AOP-Model",
        "X-AOP-System-Name",
        "X-AOP-System-Version",
        "X-AOP-Unique-Id"
      ],
      "expose_headers": [
        "Content-Length",
        "Content-Type",
        "Content-Disposition",
        "X-Content-Filename"
      ]
    },
    "security/http": {
      "sts_seconds": "31536000",
      "sts_include_subdomains": true,
      "frame_deny": true,
      "browser_xss_filter": true
    }
  }
}
